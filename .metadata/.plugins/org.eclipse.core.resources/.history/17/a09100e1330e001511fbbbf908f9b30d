var WeatherStationViewModel = function(view){
	this.view = view;
	this.services = {
		consultCityWeather: 'consultCityWeather'
	}
	
	this.consultCityWeather = function(city){
		debugger
		$.ajax({
			url: Constant.server.url+this.services.consultCityWeather, 
			data:{
				city: city
			},
			type: 'POST',
			success: function(result){
				$("#div1").html(result);
			}
		});
	}
}  